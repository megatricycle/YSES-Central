doctype html
html
  head
    title YSES Central
    link(rel='stylesheet', href='/stylesheets/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')
    link(rel='stylesheet', href='/stylesheets/chat.css')
    link(rel='stylesheet', href='/stylesheets/custom-navbar.css')
    link(rel='stylesheet', href='/stylesheets/jquery-ui.css')
    link(rel='stylesheet', href='/stylesheets/search-bar.css')
    link(rel="stylesheet",href="/stylesheets/profile-page.css")
    link(rel="stylesheet",href="/stylesheets/title-hover.css")
