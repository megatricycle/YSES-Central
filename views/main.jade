								div#nav-button-group.text-center
									div#logbook-icon.hidden-sm.hidden-md
										a(href="#",data-toggle="modal",data-target="#logbookModal")
											span.glyphicon.glyphicon-pencil
										div#logbookModal.modal.fade
											div.modal-dialog
												div.modal-content.text-center
													div.text-left.modal-header
														button.close(type="button",data-dismiss="modal")
															span(aria-hidden="true") &times;
															span.sr-only Close
														h6.modal-title Write to Logbook
