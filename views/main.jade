							span.icon-bar
							span.icon-bar
							span.icon-bar
						div#central-title
							a.ajax.navbar-brand(href="http://localhost:8080")
								img#logo.img-responsive(src="../images/logoAllWhite.png")
								h2(style="position:relative;margin:0px;top:-7px;") YSES Central
						div#search-bar.right-inner-addon.hidden-xs.hidden-sm.hidden-md
							// label(for="tags")
							i.glyphicon.glyphicon-search
							input#tags.form-control(type="text",name="mentee-1")
						div.btn-group.text-left.hidden-xs(style='float:right')
							div#navigation-buttons
								button.btn-name.dropdown-toggle(type="button",data-toggle="dropdown")
